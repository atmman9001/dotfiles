Xft.dpi: 115
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

XTerm*background: #2b2b2b
XTerm*foreground: #e7e7e7
XTerm*pointerColor: #16A085
XTerm*faceName: Fixed
XTerm*faceSize: 11

 
*background:            #272827
*foreground:            #657b83
*fading:                15
*fadeColor:             black
*cursorColor:           #16A085
*pointerColorBackground:#2B2C2B
*pointerColorForeground:#16A085


URxvt.internalBorder: 6
! special
*.foreground:   #c6c6c6
*.background:   #1f1f1f
*.cursorColor:  #c6c6c6

! black
*.color0:       #828282
*.color8:       #828282

! red
*.color1:       #cc6666
*.color9:       #cc6666

! green
*.color2:       #81a2be
*.color10:      #81a2be

! yellow
*.color3:       #f0c674
*.color11:      #f0c674

! blue
*.color4:       #81a2be
*.color12:      #81a2be

! magenta
*.color5:       #b294bb
*.color13:      #b294bb

! cyan
*.color6:       #8abeb7
*.color14:      #8abeb7

! white
*.color7:       #c5c8c6
*.color15:      #c5c8c6


Xcursor.theme: Breeze
Xcursor.size:  0

URxvt.font: xft:Source Code Pro:size=10:antialias=true:hinting=true
URxvt.geometry: 80x20
URxvt.depth: 32
URxvt.background:	[100]#101218
URxvt*scrollBar:                      false
URxvt*mouseWheelScrollPage:           false
URxvt*cursorBlink:                    true
URxvt*.foreground: 		      #c6c6c6
URxvt*cursorColor:		      #c6c6c6
URxvt*saveLines:                      5000

URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.perl-ext-common: default,clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Shift-Control-V: eval:paste_clipboard

URxvt.clipboard.autocopy: true
